{
  "name": "solito-with-tailwind",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@types/react": "^18.3.3",
    "eslint": "^8.21.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "turbo": "^2.0.9",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "native": "cd apps/expo && pnpm run start",
    "web": "cd apps/next && pnpm run next",
    "clean": "rm -rf node_modules",
    "clean:workspaces": "turbo clean",
    "tapup": "turbo run dev --filter=@codevs/ui --filter=tapup",
    "dev": "turbo watch dev",
    "codebility": "turbo run dev --filter=@codevs/ui --filter=codebility",
    "ui-add": "turbo run ui-add"
  },
  "resolutions": {
    "metro": "~0.80.8",
    "metro-resolver": "~0.80.8"
  },
  "nohoist": [
    "**/expo-router",
    "**/expo-router/**"
  ],
  "packageManager": "pnpm@8.11.0",
  "dependencies": {
    "axios": "^1.7.3"
  }
}
