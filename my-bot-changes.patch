+        if (interaction.deferred) {
+            await interaction.editReply(errorMessage);
+        }
+        else if (!interaction.replied) {
+            await interaction.reply({
+                ...errorMessage,
+                flags: [discord_js_1.MessageFlags.Ephemeral],
+            });
+        }
+    }
