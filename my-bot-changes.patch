+            name: interaction.guild?.name || "Unknown Guild",
+            active: true,
+        }, { onConflict: "id" });
+        if (guildError) {
+            console.error("‚ùå Error ensuring guild record:", guildError.message);
+        }
+        // --- Query top users from user_stats_discord (no view needed) ---
